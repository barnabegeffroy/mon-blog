{
  "name": "mon-blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "bahs build.sh",
    "preview": "vite preview",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "deploy": "bash script.sh && bash build.sh && gh-pages -d build -t true",
    "data": "bash script.sh",
    "count": "find . -path ./node_modules -prune -o -name '*.md' | grep '[.]md' | xargs wc -w",
    "spell": " for f in $(find . -path ./node_modules -prune -o -name '*.md' | grep '[.]md'); do aspell -x -c $f; done"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "vite": "^3.0.4",
    "marked": "^4.0.16",
    "mdsvex": "^0.10.5",
    "rehype-urls": "^1.1.1",
    "svelte-preprocess": "^4.10.1",
    "svg-sprite": "^1.5.4",
    "gh-pages": "^3.1.0"
  },
  "type": "module"
}
